function mettreAJourFeuille() {
  var feuilleSource = SpreadsheetApp.getActive().getSheetByName("Source");
  var feuilleDest = SpreadsheetApp.getActive().getSheetByName("Dest");

  var dataSource = feuilleSource.getDataRange().getValues();
  var dataDest = feuilleDest.getDataRange().getValues();

  // Créer un objet pour stocker les données de la feuille source
  var rowsSource = {};
  dataSource.forEach(function(row) {
    rowsSource[row[0]] = row; // Stocker la ligne dans l'objet en utilisant la première colonne comme clé
  });

  // Boucle for pour parcourir chaque ligne de la feuille de destination
  for (var i = dataDest.length - 1; i >= 0; i--) {
    var rowDest = dataDest[i];
    var key = rowDest[0];

    // Vérifier si la ligne de destination est présente dans l'objet de la feuille source
    if (rowsSource[key]) {
      // Mettre à jour les données de la ligne de destination avec les données de la ligne source
      feuilleDest.getRange(i+1, 2).setValue(rowsSource[key][1]); // Mettre à jour la deuxième colonne avec la valeur correspondante de la feuille source
      delete rowsSource[key]; // Supprimer la ligne de l'objet pour marquer comme traitée
    } else {
      feuilleDest.deleteRow(i+1); // Supprimer la ligne de destination si elle n'est pas présente dans la feuille source
    }
  }

  // Ajouter les nouvelles lignes de la feuille de destination à la feuille source
  for (var key in rowsSource) {
    feuilleSource.appendRow(rowsSource[key]); // Ajouter la ligne à la fin de la feuille source
  }
}
